# server
dev: False
sign_images: True
server_user: root
server_dir: /vagrant
updater_dir: /var/lib/asu/updater
updater_threads: 4
worker:
    - /var/lib/asu/worker

active_distros:
    - lime
    - openwrt

# database
database_type: PostgreSQL Unicode
database_address: localhost
database_user: postgres
database_pass: changeme
database_port: 5432

# folder
distro_folder: /etc/asu/distributions
download_folder: /var/cache/asu/download
