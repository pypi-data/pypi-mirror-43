{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "project_name",
    "workspace",
    "log_code",
    "log_graph",
    "auto_param_logging",
    "auto_metric_logging",
    "parse_args",
    "auto_output_logging",
    "disabled",
    "log_env_details",
    "log_git_metadata",
    "feature_toggles_overrides",
    "start_time",
    "stop_time",
    "tags"
  ],
  "properties": {
    "project_name": {
      "$id": "#/properties/project_name",
      "type": ["string", "null"],
      "title": "The Project_name Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "workspace": {
      "$id": "#/properties/workspace",
      "type": ["string", "null"],
      "title": "The Workspace Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "log_code": {
      "$id": "#/properties/log_code",
      "type": "boolean",
      "title": "The Log_code Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "log_graph": {
      "$id": "#/properties/log_graph",
      "type": "boolean",
      "title": "The Log_graph Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "auto_param_logging": {
      "$id": "#/properties/auto_param_logging",
      "type": "boolean",
      "title": "The Auto_param_logging Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "auto_metric_logging": {
      "$id": "#/properties/auto_metric_logging",
      "type": "boolean",
      "title": "The Auto_metric_logging Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "parse_args": {
      "$id": "#/properties/parse_args",
      "type": "boolean",
      "title": "The Parse_args Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "auto_output_logging": {
      "$id": "#/properties/auto_output_logging",
      "type": ["string", "null"],
      "enum": ["simple", "native", null],
      "title": "The Auto_output_logging Schema",
      "default": "",
      "examples": [
        "native"
      ],
      "pattern": "^(.*)$"
    },
    "disabled": {
      "$id": "#/properties/disabled",
      "type": "boolean",
      "title": "The Disabled Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "log_env_details": {
      "$id": "#/properties/log_env_details",
      "type": "boolean",
      "title": "The Log_env_details Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "log_git_metadata": {
      "$id": "#/properties/log_git_metadata",
      "type": "boolean",
      "title": "The Log_git_metadata Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "feature_toggles_overrides": {
      "$id": "#/properties/feature_toggles_overrides",
      "type": "object",
      "title": "The Feature_toggles_overrides Schema"
    },
    "start_time": {
      "$id": "#/properties/start_time",
      "type": "integer",
      "title": "The Start_time Schema"
    },
    "stop_time": {
      "$id": "#/properties/stop_time",
      "type": "integer",
      "title": "The Stop_time Schema"
    },
    "tags": {
      "$id": "#/properties/tags",
      "type": "array",
      "title": "The Tags Schema",
      "items": {
        "$id": "#/properties/tags/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "tag1",
          "tag2",
          "tag3"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}

