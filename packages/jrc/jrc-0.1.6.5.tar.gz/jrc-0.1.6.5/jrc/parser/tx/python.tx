TreeModel:
    imports+=Import
    nodes+=Node
;

Import:
    'from' /[a-zA-Z0-9_.]*/ 'import *'
;

Node:
    handler=ID'='name=ID'()'
    properties*=Property
    (ID'.'func=Func'()')?
;

Property:
    Single | Nested
;

Single:
    ID'.'key=ID '=' value=Value
;

Nested:
    ID'.'ID'.'key=ID '=' value=Value
;

Value:
    name=STRING
;

Func:
    'show' | 'check'
;

Comment:
    /#.*$/
;